{
	"manifest_version": 3,
	"name": "TikTok Ads+",
	"description": "Auto refresh for TikTok Ads",
	"version": "0.0.1",
	"icons": {
		"16": "logo/logo-16.png",
		"48": "logo/logo-48.png",
		"128": "logo/logo-128.png"
	},
	"permissions": [],
	"host_permissions": [
		"https://ads.tiktok.com/*"
	],
	"content_scripts": [
		{
			"run_at": "document_end",
			"js": [
				"foreground.js"
			],
			"matches": [
				"https://ads.tiktok.com/*"
			]
		}
	]
}